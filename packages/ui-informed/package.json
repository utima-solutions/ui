{
  "name": "@utima/ui-informed",
  "version": "1.0.0-alpha.1",
  "description": "Utima UI-Informed based on Utima UI and form library Informed with support for Zod validations.",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "pnpm build; storybook build"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/src/index.d.ts",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./devtools": {
      "import": "./dist/esm/devtools.js",
      "types": "./dist/src/devtools/devtools.d.ts",
      "require": "./dist/cjs/devtools.js",
      "node": "./dist/cjs/devtools.js",
      "default": "./dist/cjs/devtools.js"
    }
  },
  "keywords": [
    "utima",
    "ui",
    "informed",
    "radix",
    "shadcn",
    "react",
    "zod",
    "validation",
    "form",
    "form-library",
    "component",
    "library",
    "components"
  ],
  "author": "Jan Šimeček",
  "license": "MIT",
  "devDependencies": {
    "informed": "^4.63.0",
    "zod": "^3.24.2"
  },
  "dependencies": {
    "@utima/ui": "workspace:^",
    "class-variance-authority": "^0.7.0",
    "react-json-tree": "^0.19.0",
    "tailwind-variants": "^0.3.1",
    "type-fest": "^4.35.0"
  },
  "peerDependencies": {
    "informed": ">=4",
    "lucide-react": "*",
    "react": ">=18",
    "react-dom": ">=18",
    "zod": ">=3.0.0 <4.0.0"
  }
}
