{
  "name": "@utima/ui",
  "version": "0.0.1",
  "description": "Utima UI based on shadcn's UI and Radix Primitives with some modifications.",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "main": "./dist/src/index.js",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -w",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint -c ../../eslint.config.mjs './**/*.{js,ts,jsx,tsx,cjs,mjs}'"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js",
      "types": "./dist/src/index.js.d.ts"
    },
    "./tokens/*": {
      "import": "./dist/src/tokens/*.js",
      "default": "./dist/src/tokens/*.js",
      "types": "./dist/src/tokens/*.d.ts"
    }
  },
  "keywords": [
    "utima",
    "ui",
    "react",
    "css",
    "css-in-js",
    "component-library",
    "component",
    "library",
    "components"
  ],
  "author": "Jan Šimeček",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.46",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  }
}
